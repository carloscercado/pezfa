<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.pezfa.inventario.models.Terminado" table="terminado" schema="public" optimistic-lock="version">
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>
        <many-to-one name="produccion" class="com.pezfa.inventario.models.Produccion" fetch="select">
            <column name="produccion" not-null="true" />
        </many-to-one>
        <many-to-one name="producto" class="com.pezfa.inventario.models.Producto" fetch="select">
            <column name="producto" not-null="true" />
        </many-to-one>
        <property name="vencimiento" type="date">
            <column name="vencimiento" length="13" not-null="true" />
        </property>
        <property name="lote" type="string">
            <column name="lote" length="20" not-null="true" />
        </property>
        <property name="estado" type="java.lang.Boolean">
            <column name="estado" default="true"/>
        </property>
        <set name="ventaTerminados" table="venta_terminado" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="terminado" not-null="true" />
            </key>
            <one-to-many class="com.pezfa.inventario.models.VentaTerminado" />
        </set>
    </class>
</hibernate-mapping>
